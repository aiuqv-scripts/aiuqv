-- StarterGui.voltage stats.stats.timer.LocalScript
local function C_28()
local script = G2L["28"];
	local label = script.Parent
	local totalSeconds = 0
	
	while true do
		task.wait(1)
		totalSeconds += 1
	
		local months = math.floor(totalSeconds / (30 * 86400))
		local weeks = math.floor((totalSeconds % (30 * 86400)) / (7 * 86400))
		local days = math.floor((totalSeconds % (7 * 86400)) / 86400)
		local hours = math.floor((totalSeconds % 86400) / 3600)
		local minutes = math.floor((totalSeconds % 3600) / 60)
		local seconds = totalSeconds % 60
	
		label.Text = string.format(
			"%02dM %02dW %02dD %02d:%02d:%02d",
			months,
			weeks,
			days,
			hours,
			minutes,
			seconds
		)
	end
	
end;
task.spawn(C_28);
-- StarterGui.voltage stats.stats.sps.LocalScript
local function C_2c()
local script = G2L["2c"];
	local textLabel = script.Parent
	local player = game.Players.LocalPlayer
	local strengthStat = player:FindFirstChild("leaderstats") and player.leaderstats:FindFirstChild("Strength")
	local runService = game:GetService("RunService")
	
	if not strengthStat then
		warn("Strength stat not found in leaderstats!")
		return
	end
	
	local lastStrength = strengthStat.Value
	local totalStrengthGained = 0
	
	local function updateStrengthGained()
		local currentStrength = strengthStat.Value
		local strengthDifference = currentStrength - lastStrength
		totalStrengthGained = totalStrengthGained + strengthDifference
		lastStrength = currentStrength 
	
		textLabel.Text = " " .. totalStrengthGained
	end
	
	
	runService.RenderStepped:Connect(function()
		updateStrengthGained()
	end)
	
end;
task.spawn(C_2c);

return G2L["1"], require;
