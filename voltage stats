	local function shortFormat(n)
		if n >= 1e12 then
			return string.format("%.2fT", n / 1e12)
		elseif n >= 1e9 then
			return string.format("%.2fB", n / 1e9)
		elseif n >= 1e6 then
			return string.format("%.2fM", n / 1e6)
		elseif n >= 1e3 then
			return string.format("%.2fK", n / 1e3)
		else
			return tostring(math.floor(n))
		end
	end
	
	while true do
		index += 1
		local before = strengthStat.Value
		task.wait(1)
	
		if index <= 3 then
			label.Text = "..."
		else
			local sps = strengthStat.Value - before
			local spm = sps * 60
			label.Text = "Spm = " .. shortFormat(spm)
		end
	end
	
end;
task.spawn(C_14);
-- StarterGui.voltage stats.stats.sph.LocalScript
local function C_18()
local script = G2L["18"];
	local label = script.Parent
	local player = game:GetService("Players").LocalPlayer
	local leaderstats = player:WaitForChild("leaderstats")
	local strengthStat = leaderstats:WaitForChild("Strength")
	local index = -1
	
	local function shortFormat(n)
		if n >= 1e12 then
			return string.format("%.2fT", n / 1e12)
		elseif n >= 1e9 then
			return string.format("%.2fB", n / 1e9)
		elseif n >= 1e6 then
			return string.format("%.2fM", n / 1e6)
		elseif n >= 1e3 then
			return string.format("%.2fK", n / 1e3)
		else
			return tostring(math.floor(n))
		end
	end
	
	while true do
		index += 1
		local before = strengthStat.Value
		task.wait(1)
	
		if index <= 3 then
			label.Text = "..."
		else
			local sps = strengthStat.Value - before
			local sph = sps * 3600
			label.Text = "Sph = " .. shortFormat(sph)
		end
	end
	
end;
task.spawn(C_18);
